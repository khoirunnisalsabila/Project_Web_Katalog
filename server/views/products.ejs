<h1>Daftar Produk</h1>
<ul>
  <% products.forEach(product => { %>
    <li>
      <h3><%= product.name %></h3>
      <p><%= product.description %></p>
      <p>Harga: <%= product.price %></p>
      <img src="<%= product.image_url %>" alt="<%= product.name %>" width="100">
    </li>
  <% }) %>
</ul>


